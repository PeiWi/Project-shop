<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>BeautyMixer - Products Theme</title>


  <!-- Bootstrap core CSS -->
  <link href="../vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

  <!-- Custom fonts for this template -->
  <link href="../vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href='https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800'
    rel='stylesheet' type='text/css'>

  <!-- Custom styles for this template -->
  <link href="../css/clean-blog.min.css" rel="stylesheet">
  <script>
    function Find(id) {
      window.location.href = "/post/item?id=" + id;
    }
  </script>

</head>

<body>

  <!-- Navigation -->
  <nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
    <div class="container">
      <a class="navbar-brand mr-1" href="/">BeautyMixer</a>
      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive"
        aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
        Menu
        <i class="fas fa-bars"></i>
      </button>
      <div class="collapse navbar-collapse" id="navbarResponsive">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link" href="/">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="../about">About</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="../post">Products</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="../contact">Contact</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="../admin">Admin</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>


  <!-- Page Header -->
  <header class="masthead" style="background-image: url('../img/home-bg.jpg')">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <div class="site-heading">
          </div>
        </div>
      </div>
    </div>
  </header>

  <!-- Page Content -->
  <div class="container">

      <div class="row">

       <div class="col-lg-3">
        <div class="container-fluid">
          <h1 class="my-4">Categories</h1>
          <div id="accordion">
          <div class="list-group" id="accordion-item">
            <a button class="btn btn-primary" id="headingOne" type="button" data-toggle="collapse" data-target=".multi-collapse" aria-expanded="false" 
              aria-controls="collapseOne collapseTwo collapseThree collapseFour collapseFive collapseSix collapseSeven collapseEight">All</a>

            <button id= "headingTwo" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">Cosmetic</a>
            <button id= "headingThree" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">Care products</a>
            <button id= "headingFour" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseThree" aria-expanded="true" aria-controls="collapseThree">Remove products</a>
            <button id= "headingFive" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseFour" aria-expanded="true" aria-controls="collapseFour">Nail products</a>
            <button id= "headingSix" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseFive" aria-expanded="true" aria-controls="collapseFive">Perfume products</a>
            <button id= "headingSeven" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseSix" aria-expanded="true" aria-controls="collapseSix">Hair products</a>
            <button id= "headingEight" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseSeven" aria-expanded="true" aria-controls="collapseSeven">Hair-Care products</a>
            <button id= "headingNight" class="list-group-item collapsed list-group-item-action" data-toggle="collapse" data-target="#collapseEight" aria-expanded="true" aria-controls="collapseEight">Body products</a>
</div>
</div>
</div>

</div>

          <!-- 分類結束 -->
        <!-- /.col-lg-3 -->

        <div class="col-lg-9">

          <div id="carouselExampleIndicators" class="carousel slide my-4" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner" role="listbox">
              <div class="carousel-item active">
                <img class="d-block img-fluid" src="https://i.imgur.com/kRGv4kX.jpg" alt="First slide">
              </div>
              <div class="carousel-item">
                <img class="d-block img-fluid" src="https://i.imgur.com/kRGv4kX.jpg" alt="Second slide">
              </div>
              <div class="carousel-item">
                <img class="d-block img-fluid" src="https://i.imgur.com/kRGv4kX.jpg" alt="Third slide">
              </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>

          <div class="row">

            <% for(var i=0; i< data.length; i++) { %>
            <div class="col-lg-4 col-md-6 mb-4">


              <% if(data[i].cname == "化妝品"){%>
                <div id="collapseOne" class="collapse multi-collapse" aria-labelledby="headingOne" data-parent="#accordion">
                  <% } %>
                  
              <% if(data[i].cname == "保養品") { %>
                  <div id="collapseTwo" class="collapse multi-collapse" aria-labelledby="headingTwo" data-parent="#accordion">
                  <% } %>

                  <% if(data[i].cname == "卸妝品") { %>
                    <div id="collapseThree" class="collapse multi-collapse" aria-labelledby="headingThree" data-parent="#accordion">
                    <% } %>

                    <% if(data[i].cname == "美甲品") { %>
                      <div id="collapseFour" class="collapse multi-collapse" aria-labelledby="headingFour" data-parent="#accordion">
                      <% } %>

                      <% if(data[i].cname == "香水/體香劑") { %>
                        <div id="collapseFive" class="collapse multi-collapse" aria-labelledby="headingFive" data-parent="#accordion">
                        <% } %>

                        <% if(data[i].cname == "美髮品") { %>
                          <div id="collapseSix" class="collapse multi-collapse" aria-labelledby="headingSix" data-parent="#accordion">
                          <% } %>

                          <% if(data[i].cname == "護髮品") { %>
                            <div id="collapseSeven" class="collapse multi-collapse" aria-labelledby="headingSeven" data-parent="#accordion">
                            <% } %>

                            <% if(data[i].cname == "美體儀") { %>
                              <div id="collapseEight" class="collapse multi-collapse" aria-labelledby="headingEight" data-parent="#accordion">
                              <% } %>

                    <div class="card h-100">
                      <a value="Item" onclick="Find('<%= data[i].id %>')"><img class="card-img-top" src="https://i.imgur.com/iAUjFSJ.jpg"
                          alt="" style="cursor:pointer"></a>
                      <div class="card-body">
                        <h4 class="card-title">
                          <a value="Item" onclick="Find('<%= data[i].id %>')" style="cursor:pointer">
                            <%= data[i].pname %></a>
                        </h4>
                        <h5>NT$
                          <%= data[i].price %>
                        </h5>
                        <p class="card-text">
                          <%= data[i].detail %>
                        </p>
                      </div>
                      <div class="card-footer">
                        <small class="text-muted">&#9733; &#9733; &#9733; &#9733; &#9734;</small>
                      </div>
                    </div>
                  </div>
                </div>



                <% } %>

              </div>
              <!-- /.row -->
            </div>
          </div>
          <!-- /.col-lg-9 -->

        </div>
        <!-- /.row -->
      </div>
    </div>
    <!-- /.container -->

    <!-- Footer -->
    <footer>
      <div class="container">
            <p class="copyright text-muted">Copyright &copy; BeautyMixer 2018</p>
          </div>
        </div>
      </div>
    </footer>

    <!-- Bootstrap core JavaScript -->
    <script src="../vendor/jquery/jquery.min.js"></script>
    <script src="../vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Custom scripts for this template -->
    <script src="../js/clean-blog.min.js"></script>

</body>

</html>